{
    "name": "elephantsgroup/eg-blog",
    "description": "Yii2 blog management module",
    "keywords": ["yii", "yii2", "blog", "elephantsgroup"],
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Jalal Jaberi",
            "email": "j_jaberi@yahoo.com"
        },
        {
            "name": "Arezou Zahedi Majd",
            "email": "a_majd_8@yahoo.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "*",
        "elephantsgroup/eg-base": "dev-master",
        "elephantsgroup/eg-stat": "dev-master",
        "elephantsgroup/eg-like": "dev-master",
        "elephantsgroup/eg-follow": "dev-master",
        "elephantsgroup/eg-star-rating": "dev-master",
        "elephantsgroup/eg-comment": "dev-master",
        "elephantsGroup/eg-jdf": "dev-master",
        "elephantsGroup/eg-jdate": "dev-master",
        "kartik-v/yii2-widget-timepicker": "*",
        "2amigos/yii2-ckeditor-widget": "^1.0@dev",
        "elephantsGroup/eg-kcfinder": "dev-master",
        "kosinix/grafika": "dev-master"
    },
    "config": {
        "process-timeout": 1500,
        "secure-http": false
    },
    "autoload": {
        "psr-4": { "elephantsGroup\\blog\\": "" }
    }
}
